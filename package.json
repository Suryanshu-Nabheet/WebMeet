{
  "name": "study-room",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "Frontend",
    "Backend"
  ],
  "scripts": {
    "dev": "npm run dev -w Frontend",
    "build": "npm run build -w Frontend",
    "start": "tsx server.ts",
    "lint": "npm run lint -w Frontend",
    "server": "npm run server -w Backend",
    "server:dev": "npm run server:dev -w Backend",
    "clean": "rm -rf Frontend/.next Backend/dist",
    "clean:all": "rm -rf Frontend/.next Frontend/node_modules Backend/node_modules node_modules dist"
  },
  "devDependencies": {
    "tsx": "^4.7.0"
  }
}

